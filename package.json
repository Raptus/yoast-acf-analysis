{
  "name": "yoast-acf-analysis",
  "version": "2.0.0-dev",
  "description": "WordPress plugin that adds the content of all ACF fields to the Yoast SEO score analysis.",
  "keywords": [
    "yoast",
    "seo",
    "acf",
    "advanced",
    "custom",
    "fields",
    "analysis",
    "search",
    "engine",
    "optimization",
    "seo",
    "score"
  ],
  "homepage": "https://wordpress.org/plugins/yoast-seo-acf-analysis/",
  "bugs": {
    "url" : "https://github.com/Yoast/yoast-acf-analysis/issues",
    "email" : "support@yoast.com"
  },
  "license": "GPL-3.0",
  "repository": "Yoast/yoast-acf-analysis",
  "scripts": {
    "build": "browserify js/src/main.js -o js/yoast-acf-analysis.js",
    "watch": "watchify js/src/main.js -o js/yoast-acf-analysis.js --poll --verbose --debug",
    "chromedriver": "xvfb-run node_modules/.bin/chromedriver",
    "test": "(cd tests/system/js/ && exec ../../../node_modules/.bin/nightwatch --retries 3)",
    "test-acf4": "(cd tests/system/js/ && exec ../../../node_modules/.bin/nightwatch --retries 3 --group general --group acf4)",
    "test-acf5pro": "(cd tests/system/js/ && exec ../../../node_modules/.bin/nightwatch --retries 3  --group general --group acf5)"
  },
  "dependencies" : {},
  "devDependencies": {
    "browserify": "^14.3.0",
    "chromedriver": "^2.29.0",
    "nightwatch": "^0.9.15",
    "watchify": "^3.9.0"
  }
}
