{
  "name": "yoast-acf-analysis",
  "version": "2.0.0-dev",
  "description": "Adds the content of all ACF fields to the Yoast SEO score analysis.",
  "main": "index.js",
  "scripts": {
    "build": "browserify js/src/main.js -o js/yoast-acf-analysis.js",
    "watch": "watchify js/src/main.js -o js/yoast-acf-analysis.js --poll --verbose --debug",
    "chromedriver": "xvfb-run node_modules/.bin/chromedriver",
    "test": "(cd tests/system/js/ && exec ../../../node_modules/.bin/nightwatch)",
    "test-acf4": "(cd tests/system/js/ && exec ../../../node_modules/.bin/nightwatch --group general --group acf4)",
    "test-acf5pro": "(cd tests/system/js/ && exec ../../../node_modules/.bin/nightwatch  --group general --group acf5)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kraftner/yoast-acf-analysis.git"
  },
  "keywords": [
    "yoast",
    "seo",
    "acf",
    "advanced",
    "custom",
    "fields",
    "analysis",
    "search",
    "engine",
    "optimization",
    "seo",
    "score"
  ],
  "author": "Thomas Kr√§ftner, Marcus Forsberg & Team Yoast",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kraftner/yoast-acf-analysis/issues"
  },
  "homepage": "https://github.com/kraftner/yoast-acf-analysis#readme",
  "devDependencies": {
    "browserify": "^14.3.0",
    "chromedriver": "^2.29.0",
    "nightwatch": "^0.9.15",
    "watchify": "^3.9.0"
  }
}
